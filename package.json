{
  "name": "openapi-challecara",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "@typespec/compiler": "latest",
    "@typespec/http": "latest",
    "@typespec/openapi": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/rest": "latest",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.1.10",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.18.3"
  },
  "scripts": {
    "build": "tsp compile . --emit @typespec/openapi3 --output-dir tsp-output/schema && mkdir -p public/schema && cp tsp-output/schema/openapi.yaml public/schema/",
    "watch": "tsp compile . --emit @typespec/openapi3 --output-dir tsp-output/schema --watch",
    "start": "nodemon server.js"
  },
  "packageManager": "npm@11.6.1+sha512.ee20d21c3a2ea7ab8c409dfbc96ae17ea71b3211745047c645aa7a36ff9a29072b2095e50a2d9c29b8fbe5606688795cc2c402cbf534cc4c0365d031e87fd56b"
}
